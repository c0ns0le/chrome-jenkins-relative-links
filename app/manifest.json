{
  "name": "__MSG_appName__",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts.babel/chromereload.js",
      "scripts.babel/background.js"
    ]
  },
  "permissions": [
    "webNavigation",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*job/*/console",
        "https://*/*job/*/console"
      ],
      "js": [
        "scripts.babel/contentscript.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
